@model AffilatePackageEditModel;
@{
    ViewBag.Title = "Edit Registration";
}
    <div class="side-app">
        @*<div class="page-header">


        </div>*@
        <!-- end row -->
        <div class="row" style="margin-top:10px;">
            <div class="col-md-12">
                <div class="card m-b-20">
                    <div class="card-header">
                        <h3 class="card-title">Edit</h3>
                    </div>
                    <div class="card-body">
                        <form method="post" enctype="multipart/form-data" asp-action="Edit">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div style="text-align:center;color:red;font-size:larger;"> @Html.ValidationMessage("CustomError")</div>
                            <div class="form-row">
                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="membershipid"></label>
                                        <select name="membershipid" id="membershipid" asp-for="membershipid" class="form-control"
                                                asp-items="@(new SelectList(ViewBag.MembershipList,"id","membershipName"))">

                                            <option>--- Select  ---</option>
                                        </select>
                                        <span asp-validation-for="membershipid" class="text-danger"></span>

                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="commissionid">Percentage</label>
                                        <select name="commissionid" id="commissionid" asp-for="commissionid" class="form-control"
                                                asp-items="@(new SelectList(ViewBag.CommissionList,"id","commissionper"))">

                                            <option>--- Select  ---</option>
                                        </select>
                                        <span asp-validation-for="commissionid" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-row">

                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="amount"></label>
                                        <input type="text" asp-for="amount" class="form-control" id="name2" placeholder="Percentage">
                                        <span asp-validation-for="amount" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="gst"></label>
                                        <input type="text" asp-for="gst" class="form-control" id="name2" placeholder="Percentage">
                                        <span asp-validation-for="gst" class="text-danger"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="form-row">

                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="affilateamt"></label>
                                        <input type="text" asp-for="affilateamt" class="form-control" id="name2" placeholder="Affilate Amount">
                                        <span asp-validation-for="affilateamt" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="plethoraamt"></label>
                                        <input type="text" asp-for="plethoraamt" class="form-control" id="name2" placeholder="Plethora Amount">
                                        <span asp-validation-for="plethoraamt" class="text-danger"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="form-row">

                                <div class="form-group col-md-12">
                                    <div class="form-group">
                                        <label class="form-label" asp-for="Description"></label>
                                        <input type="text" asp-for="Description" class="form-control" id="name2" placeholder="Percentage">
                                        <span asp-validation-for="Description" class="text-danger"></span>
                                    </div>
                                </div>

                            </div>

                            <button type="submit" style="margin-right:5px;" class="btn btn-primary "><i class="fa fa-edit" style="margin-right:0.7em"></i>Update</button>
                            <a asp-action="index" class="btn btn-rounded btn-info float-right"><i class="fa fa-backward" style="margin-right:0.7em"></i>Back</a>



                        </form>
                    </div>
                </div>
            </div>
        </div>





    </div>
